{
    "compilerOptions": {
      "baseUrl": ".",
      "allowJs": true,
      "noImplicitAny": false,
      "noUnusedLocals": false,
      "noUnusedParameters": false,
      "paths": {
        "@/*": [
          "./src/*"
        ],
        "@/components/*": [
          "./src/component/*"
        ],
        "@/component/*": [
          "./src/component/*"
        ]
      },
      "skipLibCheck": true,
      "strictNullChecks": false
    },
    "files": [],
    "references": [
      {
        "path": "./tsconfig.app.json"
      },
      {
        "path": "./tsconfig.node.json"
      }
    ]
  }